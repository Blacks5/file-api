{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/lumen-framework": "5.5.*",
        "vlucas/phpdotenv": "~2.2",
        "tnew/common": "dev-master",
        "league/flysystem": "~1.0",
        "johnlui/aliyun-oss": "~2.0",
        "ramsey/uuid": "~3.6.1",
        "intervention/image": "^2.4",
        "overtrue/laravel-wechat": "^3.1",
        "qiniu/qiniu": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "optimize-autoloader": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "ssh://git@gitee.com/tnew/TnewCommon.git"
        }
    ]
}
